INSTALL=@INSTALL@

srcdir = .
top_srcdir = @top_srcdir@
VPATH = @srcdir@
prefix = @prefix@
exec_prefix = @exec_prefix@

install_sh_DATA = $(install_sh) -c -m 644
install_sh_PROGRAM = $(install_sh) -c
install_sh_SCRIPT = $(install_sh) -c
INSTALL_HEADER = $(INSTALL_DATA)

build_triplet = @build@
host_triplet = @host@
target_triplet = @target@

bindir=${prefix}/bin
libdir=${prefix}/lib

AS = @AS@
CC = @CC@
CFLAGS = @CFLAGS@ -Wall
CPPFLAGS = @CPPFLAGS@
ECHO_C = @ECHO_C@
ECHO_N = @ECHO_N@
ECHO_T = @ECHO_T@
INSTALL = @INSTALL@
INSTALL_DATA = @INSTALL_DATA@
INSTALL_PROGRAM = @INSTALL_PROGRAM@
INSTALL_SCRIPT = @INSTALL_SCRIPT@
INSTALL_STRIP_PROGRAM = @INSTALL_STRIP_PROGRAM@
LD = @LD@
LDFLAGS = @LDFLAGS@

SOURCES = \
	mat.c

OBJS=mat.o

COMPILE=$(CC) $(CFLAGS) $(CPPFLAGS)

EXECS=mat

all:	$(EXECS)

clean:
	$(RM) -f $(OBJS) $(EXECS)

mat:	  mat.o
	$(CC) -o mat mat.o

install:
	mkdir -p ${bindir}
	$(INSTALL) mat ${bindir}/

.c.o:
	$(COMPILE) -c $<

.NOEXPORT:
